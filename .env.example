# Mathpix API 설정
APP_ID=your_mathpix_app_id
APP_KEY=your_mathpix_app_key

# MongoDB 설정
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority
MONGODB_DATABASE=YourDatabaseName

# Python 경로 (AWS 서버에서 확인 후 설정)
# 예: PYTHON_BIN=/usr/bin/python3 또는 /home/ubuntu/.venvs/yourenv/bin/python
PYTHON_BIN=python3

# 서버 URL (PDF 캡처용 - AWS 서버의 실제 도메인으로 변경)
# 예: BASE_URL=http://your-domain.com 또는 http://ec2-xx-xxx-xxx-xxx.compute.amazonaws.com
BASE_URL=http://localhost:3000

# 서버 포트 (선택사항 - 기본값 3000)
PORT=3000
